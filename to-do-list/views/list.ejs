<%- include("header")  -%> 

<div class="box" id="heading">
    <h1>
        <%= listTitle %>
    </h1>
</div>



<div class="box">

    <form action="/" method="POST">
        <input placeholder="What to do next" type="text" name="newItem" autocomplete="off">
        <button type="submit" name="list" value=<%=listTitle %> >+</button>
    </form>

    <% for(var i=0; i<item.length; i++){%>
        <div class="item">
            <input type="checkbox">
            <p>
                <%= item[i]%>
            </p>
        </div>
        <% } %>


</div>

<%- include("footer") -%> 